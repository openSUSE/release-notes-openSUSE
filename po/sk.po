#~ msgstr ""
#~ "\n"
#~ "[X11]\n"
#~ "EnableHiDPI=true\n"
#~ "ServerArguments=-nolisten tcp -dpi <replaceable>DPI_VALUE</replaceable>\n"

#~ msgid ""
