#~ msgstr "到"

#~ msgid "/dev/mapper/cr_sda3  /home   ext4    acl,user_xattr,nofail 0 2"
#~ msgstr "/dev/mapper/cr_sda3 /home ext4 acl,user_xattr,nofail 0 2"

